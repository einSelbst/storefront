{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Next.js",
    "compilerOptions": {
        "composite": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "inlineSources": false,
        "isolatedModules": true,
        "moduleResolution": "node",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "strict": true
    "allowJs": true,
        "declaration": false,
        "declarationMap": false,
        "incremental": true,
        "jsx": "preserve",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "module": "esnext",
        "noEmit": true,
        "resolveJsonModule": true,
        "target": "es5",
        "outDir": ".next",
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src",
        "next-env.d.ts",
        ".next/types/**/*.ts"
    ]
}
